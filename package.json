{
  "name": "@upsetjs/jupyter_widget",
  "version": "0.1.0",
  "description": "A Jupyter Widget Library around UpSet.js",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "widgets"
  ],
  "files": [
    "lib/**/*.js",
    "dist/*.js",
    "css/*.css"
  ],
  "homepage": "https://github.com/upsetjs/upsetjs_jupyter_widget",
  "bugs": {
    "url": "https://github.com/upsetjs/upsetjs_jupyter_widget/issues"
  },
  "license": "AGPL-3.0",
  "author": {
    "name": "Samuel Gratzl",
    "email": "samu@sgratzl.com",
    "url": "https://wwww.sgratzl.com"
  },
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/upsetjs/upsetjs_jupyter_widget"
  },
  "scripts": {
    "build": "npm run build:lib && npm run build:nbextension",
    "build:labextension": "npm run clean:labextension && mkdirp upsetjs_jupyter_widget/labextension && cd upsetjs_jupyter_widget/labextension && npm pack ../..",
    "build:lib": "tsc -p tsconfig.build.json",
    "build:nbextension": "webpack -p",
    "build:all": "npm run build:labextension && npm run build:nbextension",
    "clean": "npm run clean:lib && npm run clean:nbextension",
    "clean:lib": "rimraf lib",
    "clean:labextension": "rimraf upsetjs_jupyter_widget/labextension",
    "clean:nbextension": "rimraf upsetjs_jupyter_widget/nbextension/static/index.js",
    "prepack": "npm run build:lib",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "jest --passWithNoTests",
    "watch": "npm-run-all -p watch:*",
    "watch:lib": "tsc -w",
    "watch:nbextension": "webpack -w"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^3",
    "@upsetjs/bundle": "^0.1.0"
  },
  "devDependencies": {
    "@jupyterlab/services": "^5.0.2",
    "@phosphor/application": "^1.7.3",
    "@phosphor/widgets": "^1.9.3",
    "@types/backbone": "^1.4.1",
    "@types/jest": "^25.1.4",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "@yarnpkg/pnpify": "^2.0.0-rc.18",
    "babel-eslint": "^10.1.0",
    "backbone": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "jest": "^25.1.0",
    "jest-config": "^25.1.0",
    "mkdirp": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "pnp-webpack-plugin": "^1.6.4",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "ts-jest": "25.2.0",
    "ts-loader": "^6.2.1",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "jupyterlab": {
    "extension": "lib/plugin"
  }
}
