{
  "name": "@upsetjs/jupyter_widget",
  "version": "0.1.0",
  "description": "A Jupyter Widget Library around UpSet.js",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "widgets"
  ],
  "files": [
    "lib/**/*.js",
    "dist/*.js",
    "css/*.css"
  ],
  "homepage": "https://github.com/upsetjs/upsetjs_jupyter_widget",
  "bugs": {
    "url": "https://github.com/upsetjs/upsetjs_jupyter_widget/issues"
  },
  "license": "AGPL-3.0",
  "author": {
    "name": "Samuel Gratzl",
    "email": "samu@sgratzl.com",
    "url": "https://wwww.sgratzl.com"
  },
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/upsetjs/upsetjs_jupyter_widget"
  },
  "scripts": {
    "build": "npm run build:lib && npm run build:nbextension",
    "build:labextension": "npm run clean:labextension && mkdirp upsetjs_jupyter_widget/labextension && cd upsetjs_jupyter_widget/labextension && npm pack ../..",
    "build:lib": "tsc",
    "build:nbextension": "tsdx build",
    "build:all": "npm run build:labextension && npm run build:nbextension",
    "clean": "npm run clean:lib && npm run clean:nbextension",
    "clean:lib": "rimraf lib",
    "clean:labextension": "rimraf upsetjs_jupyter_widget/labextension",
    "clean:nbextension": "rimraf upsetjs_jupyter_widget/nbextension/static/index.js",
    "prepack": "npm run build:lib",
    "lint": "tsdx lint",
    "test": "tsdx test --passWithNoTests",
    "watch": "npm-run-all -p watch:*",
    "watch:lib": "tsc -w",
    "watch:nbextension": "tsdx watch"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^1.1.10 || ^2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@phosphor/application": "^1.6.0",
    "@phosphor/widgets": "^1.6.0",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@types/jest": "^25.1.3",
    "@yarnpkg/pnpify": "^2.0.0-rc.18",
    "eslint": "^6.8.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.19.1",
    "rimraf": "^2.6.2",
    "rollup-plugin-pnp-resolve": "^2.0.0",
    "tsdx": "^0.12.3",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3"
  },
  "jupyterlab": {
    "extension": "lib/plugin"
  }
}
